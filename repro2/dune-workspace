(lang dune 3.20)

(repository
 ; Use Alice's opam repro so we can solve with the relocatable binary release
 ; of the compiler. This lets us work around issues installing the compiler
 ; with Dune to reproduce other issues with Dune package management on Windows.
 (name alice_frozen)
 (url
  git+https://github.com/alicecaml/alice-opam-repo#781db10863f3b7a3507842e88d0d3beeebd264ad))

(repository
 (name upstream_frozen)
 (url
  git+https://github.com/ocaml/opam-repository.git#db1c715121817939c63f1096b0703a5ab64c13a3))

(repository
 (name overlay_frozen)
 (url
  git+https://github.com/ocaml-dune/opam-overlays.git#2a9543286ff0e0656058fee5c0da7abc16b8717d))

(lock_dir
 (constraints
  (ocaml-system
   (= 5.3.1+relocatable)))
 (repositories upstream_frozen overlay_frozen alice_frozen)
 (solver_env
  (sys-ocaml-version 5.3.1+relocatable)))

